{
  "name": "gulp-closure-angular-seed",
  "version": "0.0.1",
  "authors": [
    "Joshua McMichael <jmcmicha@genome.wustl.edu>"
  ],
  "description": "Client libraries for the Closure-based civic workflow",
  "main": "app/js/app.js",
  "moduleType": [
    "es6"
  ],
  "keywords": [
    "civic",
    "workflow",
    "closure"
  ],
  "license": "MIT",
  "homepage": "http://civic.genome.wustl.edu/",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.4.7",
    "angular-ui-router": "~0.2.15",
    "onsenui": "https://github.com/OnsenUI/OnsenUI.git#2.0.0-beta.2"
  },
  "devDependencies": {
    "angular-mocks": "~1.4.8",
    "bind-polyfill": "~1.0.0"
  },
  "overrides": {
    "onsenui": {
      "main": [
        "build/js/onsenui.js",
        "build/js/angular-onsenui.js",
        "build/css/onsenui.css",
        "build/css/onsen-css-components.css"
      ]
    }
  }
}
